<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#"
     xml:base="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review">
        <rdfs:comment>This ontology defines concepts for the representation of customer satisfaction reviews by airline passengers. Created by Xema Belmonte Marin (jbelmontemari@uoc.edu)</rdfs:comment>
        <rdfs:label>Airline Passenger Review Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#arrives_to -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#arrives_to">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_location"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Location"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#departs_from"/>
        <rdfs:comment>A flight arrives to a destination (city, country, airport, via...)</rdfs:comment>
        <rdfs:label>arrives_to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#departs_from -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#departs_from">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_location"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Location"/>
        <rdfs:comment>A flight departs from an origin (city, country, airport, via...)</rdfs:comment>
        <rdfs:label>departs_from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#flown_on -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#flown_on">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_date"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Date"/>
        <rdfs:comment>A flight is flown on a date</rdfs:comment>
        <rdfs:label>flown_on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_aircraft -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_aircraft">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_aircraft_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Aircraft"/>
        <rdfs:comment>A flight is flown in an aircraft</rdfs:comment>
        <rdfs:label>has_aircraft</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_airline -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_airline">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Airline"/>
        <rdfs:comment>Every review is about an airline</rdfs:comment>
        <rdfs:label>has_airline</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_country -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_country">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_country_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Passenger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Country"/>
        <rdfs:comment>A passenger is a citizen of a country</rdfs:comment>
        <rdfs:label>has_country</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_date -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_date">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Date"/>
        <rdfs:comment>A property created to point out the relationship between properties with date ranges</rdfs:comment>
        <rdfs:label>has_date</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_flight_information -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_flight_information">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_flight_information_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:comment>A review has information about the flight is reviewing</rdfs:comment>
        <rdfs:label>has_flight_information</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_location -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_location">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Location"/>
        <rdfs:comment>A property created to point out the relationship between properties of a flight that have a location as a range</rdfs:comment>
        <rdfs:label>has_location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_ratings -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_ratings">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_rating_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Ratings"/>
        <rdfs:comment>An airline passenger review has at least one overall rating, but it might have other ratings</rdfs:comment>
        <rdfs:label>has_rating</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_reviewer -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_reviewer">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_reviewer_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Passenger"/>
        <rdfs:comment>The author (a passenger) of a review</rdfs:comment>
        <rdfs:label>has_reviewer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_aircraft_of -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_aircraft_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Aircraft"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:comment>The inverse property of has_aircraft</rdfs:comment>
        <rdfs:label>is_aircraft_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_country_of -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_country_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Country"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Passenger"/>
        <rdfs:comment>The inverse property of has_country</rdfs:comment>
        <rdfs:label>is_country_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_flight_information_of -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_flight_information_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:comment>The inverse property of has_flight_information</rdfs:comment>
        <rdfs:label>is_flight_information_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_rating_of -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_rating_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Ratings"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:comment>The inverse property of has_rating</rdfs:comment>
        <rdfs:label>is_rating_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_reviewer_of -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_reviewer_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Passenger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:comment>The inverse property of has_reviewer</rdfs:comment>
        <rdfs:label>is_reviewer_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#published_on -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#published_on">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_date"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Date"/>
        <rdfs:comment>A review is published on a date</rdfs:comment>
        <rdfs:label>published_on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_comment -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_comment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A data property created to be used with the text of the passenger review</rdfs:comment>
        <rdfs:label>has_comment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_rating_value -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_rating_value">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Ratings"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#positiveInteger">10</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>A data property created to be used with the value of a rating. This number is a positive integer between 1 and 10</rdfs:comment>
        <rdfs:label>has_rating_value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_title -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#has_title">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A data property created to be used with the title of the passenger review</rdfs:comment>
        <rdfs:label>has_title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_recommended -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_recommended">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Review"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>A data property created to assert if the passenger recommends the airline for the flight of the review. It is considered a data property of a review</rdfs:comment>
        <rdfs:label>is_recommended</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_verified -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#is_verified">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>A data property created to assert if the flight has been verified. This means the editorial staff have inspected a copy of an e-ticket, booking details or a boarding pass, with the passenger name confirming the trip written about in the review</rdfs:comment>
        <rdfs:label>is_verified</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Aircraft -->


    <owl:Class rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Aircraft">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Flight_Information"/>
        <rdfs:comment>Represents a manufactured passenger aircraft</rdfs:comment>
        <rdfs:label>Aircraft</rdfs:label>
        <rdfs:sameAs>https://dbpedia.org/page/Aircraft</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Airline -->


    <owl:Class rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Airline">
        <rdfs:comment>This class represents a company that provides air transport for passengers</rdfs:comment>
        <rdfs:label>Airline</rdfs:label>
        <rdfs:sameAs>https://dbpedia.org/page/Airline</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Business -->


    <owl:Class rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Business">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Type_of_Passenger"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Leisure"/>
        <rdfs:comment>Class for passengers travelling with a clear purpose of performing business related activities</rdfs:comment>
        <rdfs:label>Business</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Business_Class -->


    <owl:Class rdf:about="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Business_Class">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Seat_Type"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Economy_Class"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#First_Class"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/xema/ontologies/2024/3/Airline_Passenger_Review#Premium_Economy"/>
        <rdfs:comment>Business class is a travel class originally intended as an intermediate level of service between economy class and first class, but many airlines now offer business class as the highest level of service.</rdfs:comment>
        <rdfs:label>Business_Class</rdfs:label>
